{
  "name": "server",
  "version": "1.0.0",
  "description": "recordnise-server",
  "main": "index.js",
  "scripts": {
    "start": "node .",
    "initdev": "npm run createDB && npm run migrate && npm run seed",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "createDB": "npx sequelize-cli db:create",
    "dropDB": "npx sequelize-cli db:drop",
    "resetDB": "npm run dropDB && npm run initdev",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "template",
    "express",
    "jwt",
    "sequelize"
  ],
  "author": "Stacey Miller",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/vision": "^1.11.0",
    "bcrypt": "^4.0.1",
    "cors": "^2.8.5",
    "disconnect": "^1.2.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.3",
    "pg": "^8.0.2",
    "sequelize": "^5.21.6",
    "sequelize-cli": "^5.5.1"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true
  }
}
